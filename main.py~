import time
from selenium import webdriver
from selenium.webdriver import Keys
from selenium.webdriver.common.by import By

browser = webdriver.Firefox()

url = "https://ru.wikipedia.org/wiki/%D0%97%D0%B0%D0%B3%D0%BB%D0%B0%D0%B2%D0%BD%D0%B0%D1%8F_%D1%81%D1%82%D1%80%D0%B0%D0%BD%D0%B8%D1%86%D0%B0"

browser.get(url)

assert "Википедия" in browser.title

search_box = browser.find_element(By.ID, "searchInput")
find_text = input("Введите текст для поиска: ").lower()
search_box.send_keys(find_text)
search_box.send_keys(Keys.RETURN)
paragraphs = browser.find_elements(By.TAG_NAME, "p")

for paragraph in paragraphs:
    print(paragraph.text)
    input("Для продолжения нажмите Enter")

hatnotes = []
for element in browser.find_elements(By.TAG_NAME, "div")
    cl = element.get_attribute("class")
    if cl == "hatnote navigation-not-searchable":
        hatnotes.append(element)
print(hatnotes)

link = hatnotes.find_element(By.TAG_NAME, "a").get_attribute("href")
browser.get(link)
time.sleep(5)

browser.quit()